<j:jelly xmlns:j="jelly:core"
	xmlns:d="jelly:define"
    xmlns:l="/lib/layout"
    xmlns:t="/lib/hudson"
	xmlns:f="/lib/form"
    xmlns:i="jelly:fmt">
    <t:summary icon="/plugin/pmccabe/icons/pmccabe-48.png">
    	<a href="${it.urlName}">${%PmccabeReport}:</a>
    	<br></br>
		<j:if test="${!(it.report == null)}">
			<ul>
				${it.details}
			</ul>
		</j:if>
	</t:summary>
</j:jelly>